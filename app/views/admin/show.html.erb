<aside class="bg-dark">
</aside>
<br>
<div class="text-center">
  <table class="table" style="width:1500px">
    <thead>
      <td>ID</td>
      <td>名前</td>
      <td>Email</td>
      <td>服の数</td>
      <td>ログイン</td>
      <td>Station</td>
      <td>Ward</td>
      <td>Created_at</td>
      <td>Updated_at</td>
      <td>サクジョ</td>
    </thead>
    <% @users.each do |u| %>
      <tr>
        <td><%= link_to "#{u.id}",user_path(u.id) %></td>
        <td><%= link_to "#{u.family_name}#{u.first_name}",user_path(u.id) %></td>
        <td><%= u.email %></td>
        <td><%= u.wears.count %></td>
        <td><%= u.sign_in_count %></td>
        <td><%= link_to "#{u.station.name}",station_path(u.station.id) %></td>
        <td><%= "#{u.ward.name}" %></td>
        <td><%= u.created_at %></td>
        <td><%= u.updated_at %></td>
        <td><%= link_to "delete", user_path(u.id), method: :delete, :class => "btn btn-primary" %></td>
      </tr>
    <% end %>
    <%= paginate @users %>
  </table>
</div>